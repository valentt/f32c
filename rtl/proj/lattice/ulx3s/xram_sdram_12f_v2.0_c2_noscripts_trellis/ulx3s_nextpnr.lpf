# ULX3S LPF for nextpnr-ecp5 (xram_sdram c2)
# Based on ulx3s_v20.lpf, stripped of unsupported IOBUF commands
# For f32c with SDRAM, LEDs, UART, SPI Flash, SD card, GPDI

# Input Clock (25 MHz)
LOCATE COMP "clk_25mhz" SITE "G2";
FREQUENCY PORT "clk_25mhz" 25 MHZ;

# PLL Generated Clocks
FREQUENCY NET "clk" 100 MHZ;

# UART (directly on ftdi pins)
LOCATE COMP "ftdi_txd" SITE "M1";
LOCATE COMP "ftdi_rxd" SITE "L4";

